        .pos 0
        irmovq stack, %rsp
        call main
        halt

        .align 8
ele1:
        .quad 0x00a
        .quad ele2
ele2:
        .quad 0x0b0
        .quad ele3
ele3:
        .quad 0xc00
        .quad 0

main:
        irmovq ele1, %rdi
        call rsum
        ret

rsum:
        andq %rdi, %rdi
        je base
        irmovq $8, %rdx
        mrmovq (%rdi), %rsi
        addq %rdx, %rdi
        mrmovq (%rdi), %rdi
        pushq %rsi
        call rsum
        popq %rsi
        addq %rsi, %rax
        ret
base:
        xorq %rax, %rax
        ret

        .pos 0x200
stack:
